{
  "name": "cli",
  "version": "0.0.4",
  "description": "An open-source, the most secure and frictionless way to share and manage app secrets across teams.",
  "author": "Envless",
  "bin": {
    "envless": "./bin/run"
  },
  "homepage": "https://envless.dev",
  "license": "AGPL-3.0",
  "main": "dist/index.js",
  "repository": "https://github.com/envless/envless",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@47ng/cloak": "^1.1.0",
    "@clack/prompts": "^0.6.3",
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^3.1.8",
    "@makepro-x/keytar": "^7.10.2",
    "add": "^2.0.6",
    "axios": "^1.4.0",
    "better-opn": "^3.0.2",
    "copy-paste": "^1.3.0",
    "downloads-folder": "^3.0.3",
    "execa": "^7.1.1",
    "js-yaml": "^4.1.0",
    "keytar": "^7.9.0",
    "kleur": "^4.1.5",
    "openpgp": "^5.7.0",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@types/node": "^20.4.4",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.3",
    "vitest": "^0.30.0"
  },
  "oclif": {
    "additionalVersionFlags": [
      "-v",
      "version"
    ],
    "bin": "envless",
    "dirname": "envless",
    "commands": "./dist/commands",
    "topicSeparator": " ",
    "topics": {},
    "update": {
      "s3": {
        "xz": true,
        "bucket": "cli.envless.dev"
      }
    }
  },
  "scripts": {
    "cli": "yarn build && ./bin/run",
    "build": "shx rm -rf dist && tsc -b",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "vitest",
    "version": "oclif readme && git add README.md",
    "check:env": "node -e 'console.log(process.env)'"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/envless/envless/issues",
  "keywords": [
    "envless, security, e2ee, .env, dotenv"
  ],
  "types": "dist/index.d.ts"
}
